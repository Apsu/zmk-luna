#define COMBO(NAME, BINDINGS, KEYPOS, LAYERS, MS) \
  combo_##NAME { \
    bindings = <BINDINGS>; \
    key-positions = <KEYPOS>; \
    layers = <LAYERS>; \
    timeout-ms = <MS>; \
    slow-release = true; \
  };

combos {
  compatible = "zmk,combos";

// Alpha bichords
  COMBO(    c, &kp     C, 10 11, base, 50)
  COMBO(    w, &kp     W, 10 12, base, 50)
  COMBO(    n, &kp     N, 10 13, base, 50)
  COMBO(minus, &kp MINUS, 10 16, base, 50)
  COMBO(equal, &kp EQUAL, 10 17, base, 50)
  COMBO( lbkt, &kp  LBKT, 10 18, base, 50)
  COMBO( rbkt, &kp  RBKT, 10 19, base, 50)

  COMBO(    d, &kp     D, 11 12, base, 50)
  COMBO(    l, &kp     L, 11 13, base, 50)
  COMBO(    q, &kp     Q, 11 16, base, 50)
  COMBO(    j, &kp     J, 11 17, base, 50)
  COMBO(    y, &kp     Y, 11 18, base, 50)
  COMBO(    x, &kp     X, 11 19, base, 50)

  COMBO(    m, &kp     M, 12 13, base, 50)
  COMBO(    f, &kp     F, 12 16, base, 50)
  COMBO(    k, &kp     K, 12 17, base, 50)
  COMBO(    p, &kp     P, 12 18, base, 50)
  COMBO(    g, &kp     G, 12 19, base, 50)

  COMBO(  sqt, &kp   SQT, 13 16, base, 50)
  COMBO( semi, &kp  SEMI, 13 17, base, 50)
  COMBO(    b, &kp     B, 13 18, base, 50)
  COMBO(    v, &kp     V, 13 19, base, 50)

  COMBO(    z, &kp     Z, 16 17, base, 50)
  COMBO(  dqt, &kp   DQT, 16 18, base, 50)
  COMBO(comma, &kp COMMA, 16 19, base, 50)

  COMBO(    u, &kp     U, 17 18, base, 50)
  COMBO(  dot, &kp   DOT, 17 19, base, 50)

  COMBO( fslh, &kp  FSLH, 18 19, base, 50)

// Mods
